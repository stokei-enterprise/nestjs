{
  "name": "@stokei/nestjs",
  "version": "1.0.5",
  "description": "Stokei NestJS library.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc -p tsconfig.json -w",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "prepare": "tsc -p tsconfig.json",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stokei/nestjs"
  },
  "keywords": [
    "NestJS",
    "Stokei"
  ],
  "author": "Douglas Pereira Bernardes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stokei/nestjs/issues"
  },
  "homepage": "https://github.com/stokei/nestjs#README",
  "peerDependencies": {
    "@nestjs/common": ">=8.4.0",
    "@nestjs/core": ">=8.4.0",
    "@nestjs/graphql": ">=10.0.6",
    "graphql": ">=16.3.0"
  },
  "dependencies": {
    "@apollo/gateway": "^0.48.1",
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "nanoid": "^3.1.30",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.2"
  }
}
